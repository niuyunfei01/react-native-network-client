{
  "presets": [
    "module:metro-react-native-babel-preset",
    "@babel/preset-flow"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/transform-runtime",
      {
        "helpers": true,
        "regenerator": false
      }
    ],
    "jsx-control-statements",
    "lodash"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console",
        [
          "import",
          {
            "libraryName": "@ant-design/react-native"
          }
        ]
      ]
    }
  }
}
